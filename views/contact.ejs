      <!-- form -->
      <div class="container-fluid">
        <div class="contactcard mb-3">
          <div class="row mt-5">
            <div class="col-lg-4 col-md-6">
              <img src="contact book pic.png" class="img-fluid " alt="...">
            </div>
            <div class="col-lg-8 col-md-6">
              <div>
                <h3 class="getintouch fw-semibold mt-lg-5 mt-md-3 mt-3 ms-lg-5 text-center text-lg-start text-md-start">Get In Touch</h3>
              </div>
                
              <form action="/api/post" method="post">

                <div class=" col-lg-10 col-md-10 mt-2 ms-lg-5 mt-md-2 col-11 ms-lg-3 ms-3 ms-md-0">
                  <input type="text" class="form-control form mt-lg-4 mt-4" id="validationCustom01" value="" placeholder="Enter your name" name="firstname" required>
                </div>

                <div class=" col-lg-10 col-md-10 mt-4 ms-lg-5 mt-md-2 col-11 ms-lg-3 ms-3 ms-md-0">
                  <input type="email" class="form-control mt-3" id="exampleFormControlInput1" value="" placeholder="Enter your email" name="email" required>
                </div>

                <div class=" col-lg-10 col-md-10 mt-4 ms-lg-5 mt-md-2 col-11 ms-lg-3 ms-3 ms-md-0">
                  <input type="text" class="form-control mt-lg-4 mt-3" id="validationCustom01" value="" placeholder="Enter your number" name="phonenumber" required>
                </div>

                <div class=" col-lg-10 col-md-10 mt-4 mt-md-2 ms-lg-5 col-11 ms-lg-3 ms-3 ms-md-0">
                  <input type="text" class="form-control mt-lg-4 mt-3 pb-5" id="validationCustom01" value="" placeholder="Message" name="message" required>
                </div>

                <div class="ms-lg-5 text-center text-lg-start text-md-start">
                  <button class="submit btn-lg fw-semibold rounded-1 fs-5 ps-4 pe-4 mt-md-5 mt-lg-5 mt-5 mb-4" type="submit">SUBMIT</button>
                </div>
                
              </form>
                
            </div>
          </div>
        </div>
